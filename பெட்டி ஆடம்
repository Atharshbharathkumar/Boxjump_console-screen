def dum():
    sombu()
    clea(flamea)
    
    def clear_screen():
        os.system('clear' if os.name == 'posix' else 'cls')
        def on_press(key):
            global frame, continue_loop

            if key == keyboard.KeyCode(char='w'):
                continue_loop = False

            if continue_loop:
                pass

    def on_release(key):
        pass
    

    frames = [
        '''






                                    
     
                            [']
        _______________________/\/\______________________________________









        ''','''






                                    
                             [']
                            
        _______________________/\/\______________________________________









