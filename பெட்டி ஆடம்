import time
from pynput import keyboard
import os

stop_flag = False
def on_key_release(key):
    global stop_flag
    if key == keyboard.Key.enter:
        
        stop_flag = True
        return False

def print_lines_from_files(file1, file2, file3, file4, file5):
    while not stop_flag:
        with open('box dance.txt', 'r', encoding='utf-8') as file1, open('bb.txt', 'r', encoding='utf-8') as file2, open('bb1.txt', 'r', encoding='utf-8') as file3, open('bb2.txt', 'r', encoding='utf-8') as file4, open('bb3.txt', 'r', encoding='utf-8') as file5:
            line1 = file1.read()
            line2 = file2.read()
            line3 = file3.read()
            line4 = file4.read()
            line5 = file5.read()

            if not line1 and not line2 and not line3 and not line4 and not line5:
                break

            print(line1, end='')
            print(line2, end='')
            print(line3, end='')
            print(line4, end='')
            print(line5, end='')

            time.sleep(0.2)

def main():
    with open('box dance.txt', 'r', encoding='utf-8') as file1, \
         open('bb.txt', 'r', encoding='utf-8') as file2, \
         open('bb1.txt', 'r', encoding='utf-8') as file3, \
         open('bb2.txt', 'r', encoding='utf-8') as file4, \
         open('bb3.txt', 'r', encoding='utf-8') as file5:

        print("Press Enter to stop the loop.")
        
        listener = keyboard.Listener(on_release=on_key_release)
        listener.start()

        print_lines_from_files(file1, file2, file3, file4, file5)

        listener.stop()

if __name__ == "__main__":
    main()



def sto():

    stop_loop = False


    def start_loop():
        nonlocal stop_loop
        count = 0
        while not stop_loop:
            print('''






                  ___________________________                  
                  |                         |
                  |  CONTROLS:              |
                  |  ENTER=To ENTER THE GAME|
                  |       S=TO LAND         |
                  |       W=to Jump         |
                   -------------------------








        ''')
            count += 1


    def on_key_press(key):
        nonlocal stop_loop
        if key == keyboard.Key.enter:
            stop_loop = True
            


    listener = keyboard.Listener(on_press=on_key_press)
    listener.start()


    start_loop()


    listener.stop()

sto()


