                                  [']
        _______________________/\/\______________________________________









        ''','''





 
                                    
                                
                                  
        _______________________/\/\_[']__________________________________









        '''
    ]

    def clear_screen():
        os.system('clear' if os.name == 'posix' else 'cls')

    def print_pattern_frame(frame_index):
        clear_screen()
        pattern_frame = frames[frame_index]
        print(pattern_frame)
        time.sleep(0.1)

    def on_press(key):
        nonlocal continue_loop

        if key == keyboard.KeyCode(char='s'):
            continue_loop = False

    def on_release(key):
        pass

    frame = 0
    continue_loop = True

    listener = keyboard.Listener(on_press=on_press, on_release=on_release)
    listener.start()

    try:
        while continue_loop:
            print_pattern_frame(frame)
            frame = (frame + 1) % len(frames)
            time.sleep(0.1)
    except KeyboardInterrupt:
        pass  

    listener.stop()
    clear_screen()
    samsa()  



 
        
def clear_screen():
    os.system('clear' if os.name == 'posix' else 'cls')

frames = ['''






                                    
     
                            [']
        _______________________/\________________________________________









        ''','''






                                    
                             [']
                            
        _______________________/\________________________________________









        ''','''





 
                              [']      
                             
                            
        _______________________/\________________________________________









        ''','''





 
                                    
                                [']
                            
        _______________________/\________________________________________









        ''','''





 
                                    
                                
                                  [']
        _______________________/\________________________________________









        ''','''





 
                                    
                                
                                  
        _______________________/\___[']__________________________________









        '''
          
]



frame = 0
continue_loop = True

def print_pattern_frame(frame_index):
    clear_screen()
    pattern_frame = frames[frame_index]
    print(pattern_frame)
    time.sleep(0.1)

def on_press(key):
    global frame, continue_loop

    if key == keyboard.KeyCode(char='s'):
        continue_loop = False

    if continue_loop:
        pass

def on_release(key):
    pass

listener = keyboard.Listener(on_press=on_press, on_release=on_release)
listener.start()    

while continue_loop:
    print_pattern_frame(frame)
    frame = (frame + 1) % len(frames) 
    time.sleep(0.1)  

listener.stop()


if __name__ == "__main__":
    
    
    dum()
    sama()

