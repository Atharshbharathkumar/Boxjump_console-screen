def clea(flame_list):
    stop_loop = False
    
    def start_loop():
        count = 0
        while not stop_loop:
            time.sleep(0.1)
            for i in flame_list:
                time.sleep(0.1)
                print(i)
                count += 1
    
    def on_key_press(key):
        nonlocal stop_loop
        if key == keyboard.KeyCode(char='w'):
            stop_loop = True
    
    listener = keyboard.Listener(on_press=on_key_press)
    listener.start()
    
    start_loop()
